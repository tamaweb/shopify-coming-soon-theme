{% if settings.iconset == 'option_1' %}
  {% render 'icon-set_1',
    id: id,
    style: style,
    icon_id: icon_id,
    icon_class: icon_class,
    url: url,
    link_class: link_class,
    tabindex: tabindex
  %}

